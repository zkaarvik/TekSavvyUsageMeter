{
  "manifest_version": 2,

  "name": "TekSavvy Usage Meter",
  "description": "Display your internet usage for the current month",
  "version": "1.0",

  "permissions": [
    "storage",
    "https://api.teksavvy.com/"
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  }
}