{
  "manifest_version": 2,

  "name": "TekSavvy Usage Meter",
  "description": "Display your current data usage for the month",
  "version": "0.1",

  "permissions": [
    "storage",
    "https://api.teksavvy.com/"
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  }
}